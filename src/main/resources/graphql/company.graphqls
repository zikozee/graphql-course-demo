    name: String!,
    rcNumber: String!,
    headOffice: String!,
    country: String!
}


enum BusinessType {
    TELECOMS, BANKING, EDUCATION
}


input CompanyInput {
    name: String!,
    rcNumber: String!,
    headOffice: String!,
    country: String!
}

type AllCompanyResponse implements Response{
